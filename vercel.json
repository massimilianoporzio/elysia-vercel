{
  "installCommand": "bun install", 
  "buildCommand": "bun run build",
  "functions": {
    "src/index.ts": {
      "runtime": "nodejs@20.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/src/index.ts"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [{ "key": "X-Powered-By", "value": "Elysia on Vercel" }]
    }
  ]
}